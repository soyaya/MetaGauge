{
  "name": "improve",
  "version": "1.0.0",
  "description": "Multi-Chain Smart Contract Analytics Platform with API",
  "main": "src/api/server.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node src/api/server.js",
    "dev": "nodemon",
    "quick-start": "node start.js",
    "analyze": "node src/main.js",
    "analyze:competitors": "node src/main.js --competitors",
    "analyze:comparative": "node src/main.js --comparative",
    "reports": "node report-manager.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=tests/setup/jest.config.js",
    "test:unit": "npm test -- tests/unit",
    "test:integration": "npm test -- tests/integration",
    "test:property": "npm test -- tests/property",
    "test:performance": "npm test -- tests/performance",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:api": "node test/api-test.js",
    "test:interaction": "node test-contract-interaction-fetching.js",
    "test:lisk-optimization": "node test-lisk-interaction-optimization.js",
    "test:interaction-suite": "node test-interaction-suite.js",
    "demo:interaction": "node test-interaction-demo.js",
    "test:multi-chain-indexing": "node test-multi-chain-indexing-scenarios.js",
    "test:complete-indexing": "node test-complete-multi-chain-indexing.js",
    "test:indexing-scenarios": "node test-multi-chain-indexing-scenarios.js",
    "test:cross-chain-competitive": "node test-cross-chain-competitive-analysis.js",
    "test:subscription": "node test-subscription-system.js",
    "test:subscription-integration": "node test-complete-subscription-integration.js",
    "test:rainbowkit": "node test-rainbowkit-integration.js",
    "db:setup": "node scripts/setup-database.js",
    "db:test": "node scripts/test-connection.js",
    "db:schema": "node scripts/create-schema.js",
    "db:verify": "node scripts/verify-schema.js",
    "db:migrate": "node scripts/migrate-data.js",
    "db:verify-migration": "node scripts/verify-migration.js",
    "db:test-storage": "node scripts/test-postgres-storage.js",
    "db:reset": "node scripts/reset-database.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\""
  },
  "keywords": [
    "blockchain",
    "analytics",
    "defi",
    "smart-contracts",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.38.0",
    "@google/generative-ai": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "d3-scale": "^4.0.2",
    "dotenv": "^17.2.3",
    "dune-client": "^0.9.0",
    "ethers": "^6.16.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.21",
    "ml-matrix": "^6.12.1",
    "moment": "^2.30.1",
    "mongoose": "^8.21.1",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.11",
    "pg": "^8.18.0",
    "simple-statistics": "^7.8.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "fast-check": "^4.5.3",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2"
  }
}
