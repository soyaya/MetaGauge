# =============================================================================
# STARTUP INTELLIGENCE ANALYTICS ENGINE - CONFIGURATION TEMPLATE
# =============================================================================
# Copy this file to .env and fill in your values

# -----------------------------------------------------------------------------
# TARGET CONTRACT CONFIGURATION
# -----------------------------------------------------------------------------
CONTRACT_ADDRESS=0xYourContractAddress
CONTRACT_CHAIN=ethereum
CONTRACT_ABI_PATH=./abis/your-contract.json
CONTRACT_NAME=YourStartup

# -----------------------------------------------------------------------------
# COMPETITOR CONTRACTS (5 Competitors)
# -----------------------------------------------------------------------------
COMPETITOR_1_ADDRESS=0xCompetitor1Address
COMPETITOR_1_ABI_PATH=./abis/competitor-1.json
COMPETITOR_1_NAME=Competitor1Name
COMPETITOR_1_CHAIN=ethereum

COMPETITOR_2_ADDRESS=0xCompetitor2Address
COMPETITOR_2_ABI_PATH=./abis/competitor-2.json
COMPETITOR_2_NAME=Competitor2Name
COMPETITOR_2_CHAIN=ethereum

COMPETITOR_3_ADDRESS=0xCompetitor3Address
COMPETITOR_3_ABI_PATH=./abis/competitor-3.json
COMPETITOR_3_NAME=Competitor3Name
COMPETITOR_3_CHAIN=ethereum

COMPETITOR_4_ADDRESS=0xCompetitor4Address
COMPETITOR_4_ABI_PATH=./abis/competitor-4.json
COMPETITOR_4_NAME=Competitor4Name
COMPETITOR_4_CHAIN=ethereum

COMPETITOR_5_ADDRESS=0xCompetitor5Address
COMPETITOR_5_ABI_PATH=./abis/competitor-5.json
COMPETITOR_5_NAME=Competitor5Name
COMPETITOR_5_CHAIN=ethereum

# -----------------------------------------------------------------------------
# RPC ENDPOINTS - Multi-URL Failover Configuration
# -----------------------------------------------------------------------------
# Enhanced RPC clients support multiple URLs per chain for automatic failover
# Configure 3 URLs per chain for optimal reliability (minimum 1 required)
# Failover logic: tries each URL up to 3 times before moving to next URL

# Ethereum RPC Endpoints (3 URLs recommended)
ETHEREUM_RPC_URL1=https://ethereum-rpc.publicnode.com
ETHEREUM_RPC_URL2=https://eth.nownodes.io/YOUR_API_KEY
ETHEREUM_RPC_URL3=https://eth.llamarpc.com

# Lisk RPC Endpoints (3 URLs recommended)
LISK_RPC_URL1=https://lisk.drpc.org
LISK_RPC_URL2=https://lisk.gateway.tenderly.co/2o3VKjmisQNOJIPlLrt6Ye
LISK_RPC_URL3=https://rpc.api.lisk.com

# Starknet RPC Endpoints (3 URLs recommended)
STARKNET_RPC_URL1=https://rpc.starknet.lava.build
STARKNET_RPC_URL2=https://starknet-rpc.publicnode.com
STARKNET_RPC_URL3=https://starknet-mainnet.infura.io/v3/YOUR_INFURA_KEY

# Legacy/Other Chain Endpoints (for backward compatibility)
POLYGON_RPC_URL=https://matic.nownodes.io/YOUR_API_KEY
BASE_RPC_URL=https://base.nownodes.io/YOUR_API_KEY
ARBITRUM_RPC_URL=https://arb.nownodes.io/YOUR_API_KEY
OPTIMISM_RPC_URL=https://optimism.nownodes.io/YOUR_API_KEY

# -----------------------------------------------------------------------------
# DATABASE CONFIGURATION
# -----------------------------------------------------------------------------
DATABASE_TYPE=postgres

# MongoDB
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/
MONGO_DB=startup_analytics

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=startup_analytics
POSTGRES_USER=your_user
POSTGRES_PASSWORD=your_password
POSTGRES_URL=postgresql://user:password@localhost:5432/startup_analytics

# -----------------------------------------------------------------------------
# ANALYSIS PARAMETERS
# -----------------------------------------------------------------------------
COHORT_PERIOD_DAYS=7
RETENTION_WINDOWS=7,14,30,60,90
WHALE_THRESHOLD_ETH=10
CHURN_THRESHOLD_DAYS=30

# Smart Block Range Search Configuration (Orbiter Finance Strategy)
# Options: quick, standard, comprehensive, bridge
SEARCH_STRATEGY=standard
ANALYSIS_BLOCK_RANGE=1000

# Smart Search Parameters
SMART_SEARCH_STOP_ON_LOW_ACTIVITY=true
SMART_SEARCH_MAX_BLOCKS=2000000
SMART_SEARCH_MIN_ACTIVITY_THRESHOLD=5
SMART_SEARCH_HIGH_ACTIVITY_THRESHOLD=10

# UX Grade Thresholds
UX_GRADE_A_COMPLETION=0.9
UX_GRADE_A_FAILURE=0.05
UX_GRADE_B_COMPLETION=0.8ing
UX_GRADE_B_FAILURE=0.1
UX_GRADE_C_COMPLETION=0.7
UX_GRADE_C_FAILURE=0.15
UX_GRADE_D_COMPLETION=0.6
UX_GRADE_D_FAILURE=0.2

# -----------------------------------------------------------------------------
# OUTPUT CONFIGURATION
# -----------------------------------------------------------------------------
OUTPUT_DIR=./reports
OUTPUT_FORMATS=json,csv,markdown

# -----------------------------------------------------------------------------
# PERFORMANCE & CACHING
# -----------------------------------------------------------------------------
MAX_CONCURRENT_REQUESTS=10
CACHE_ENABLED=true
CACHE_TTL_HOURS=24
CACHE_DIR=./cache

# -----------------------------------------------------------------------------
# LOGGING
# -----------------------------------------------------------------------------
LOG_LEVEL=info
LOG_FILE=./logs/analytics.log

# -----------------------------------------------------------------------------
# SERVER CONFIGURATION
# -----------------------------------------------------------------------------
PORT=5000
